<AkClipboard
  @onSuccess={{this.handleCopySuccess}}
  @onError={{this.handleCopyError}}
  as |ac|
>
  <AkIconButton
    data-clipboard-text={{@personalToken.key}}
    id={{ac.triggerId}}
    @size='small'
  >
    <AkIcon @iconName='content-copy-outline' @color='info' />
  </AkIconButton>
</AkClipboard>