<div local-class='table-row' data-test-table-row data-test-table-row-id={{@id}}>
  <div
    local-class='platform platform-{{@amApp.platform}}'
    data-test-table-row-item
    data-test-table-row-platform={{@amApp.platform}}
  >
    {{fa-icon @amApp.platform}}
  </div>
  <div local-class='app-name-container' data-test-table-row-item>
    <span local-class='app-namespace' data-test-table-row-app-namespace>
      {{this.project.packageName}}
    </span>
    <span local-class='app-name' data-test-table-row-app-name>
      {{this.project.lastFile.name}}
    </span>
  </div>
  <div
    local-class='production-version'
    data-test-table-row-item
    data-test-table-row-prod-version
  >
    {{this.latest_am_app_version.version}}
  </div>
  <div
    local-class='last-scanned-version'
    data-test-table-row-item
    data-test-table-row-version
  >
    {{this.project.lastFile.version}}
  </div>
  <div
    local-class='project-status'
    data-test-table-row-item
    data-test-table-row-status
  >
    <AppMonitoring::Table::Status @status={{this.status}} />
  </div>
</div>