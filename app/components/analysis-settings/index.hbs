<AkTypography @variant='h5' @color='textPrimary'>
  {{t 'showUntestedAnalysis'}}
</AkTypography>

<AkFormControlLabel
  local-class='untested-analysis-toggle'
  @label={{t 'otherTemplates.showUntestedAnalysis'}}
  as |fcl|
>
  <AkToggle
    class='ml-0'
    @checked={{this.unknownAnalysisStatus.status}}
    @size='small'
    @disabled={{this.toggleUnknownAnalysis.isRunning}}
    @onClick={{this.showUnknownAnalysis}}
  />
</AkFormControlLabel>