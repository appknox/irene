<Admin::CommonPageHeader
  @title={{t 'userManagement'}}
  @subtitle='Lorem ipsum dolor sit amet consectetur. Amet habitant a risus mi enim amet arcu'
/>

<AkTabs class='mb-3' as |Akt|>
  {{#each this.tabItems as |item|}}
    <Akt.tabItem
      @id={{item.id}}
      @badgeBackground={{true}}
      @route={{item.route}}
      data-test-admin-user-tabs='{{item.id}}-tab'
    >
      {{item.label}}
    </Akt.tabItem>
  {{/each}}
</AkTabs>