<AkStack
  @alignItems='center'
  @justifyContent='center'
  @width='full'
  class='mb-4'
  data-test-security-nav-menu
>
  <AkTabs as |Akt|>
    {{#each this.menuItems as |item|}}
      <Akt.tabItem
        data-test-security-nav-menu-item='{{item.id}}'
        @id={{item.id}}
        @route={{item.route}}
        @currentWhen={{item.currentWhen}}
      >
        {{item.label}}
      </Akt.tabItem>
    {{/each}}
  </AkTabs>
</AkStack>

{{yield}}