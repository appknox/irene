<AkStack @alignItems='center'>
  <span
    class='mr-2'
    local-class='team-project-icon-{{@project.platformIconClass}}'
  >
    <AkIcon
      @iconName={{@project.platformIconClass}}
      local-class='team-project-icon'
    />
  </span>

  <AkTypography @noWrap={{true}} title={{@project.packageName}}>
    {{@project.packageName}}
  </AkTypography>
</AkStack>