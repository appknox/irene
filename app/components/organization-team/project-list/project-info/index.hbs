<AkStack @alignItems='center' @spacing='2'>
  <AkIcon
    @iconName='{{this.teamProject.platformIconClass}}'
    local-class='team-project-icon team-project-icon-{{this.teamProject.platformIconClass}}'
  />

  <AkLink
    @route='authenticated.dashboard.project.files'
    @model={{@project.id}}
    @noWrap={{true}}
    @title={{this.teamProject.packageName}}
  >
    {{this.teamProject.packageName}}
  </AkLink>
</AkStack>