= file-overview2 [
  file=file
  profileId=file.project.activeProfileId
  showMoreDetails=showMoreDetails
  showIcons=true
  click=(action "toggleFileDetails")
  showCompare=showCompare
  fileOld=fileOld
]

if showMoreDetails
  = file-extra-details [
    file=file
    project=file.project
    showInfo=false
    colspan="3"
  ]
