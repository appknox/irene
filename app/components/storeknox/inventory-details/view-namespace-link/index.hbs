<AkLink
  @color='primary'
  @route='authenticated.dashboard.organization.namespaces'
  @underline='always'
  @fontWeight='bold'
  target='_blank'
  rel='noopener noreferrer'
  {{style textTransform='capitalize'}}
  data-test-storeknoxInventoryDetails-viewNamespacesLink
  ...attributes
>
  <:leftIcon>
    <AkIcon
      @color='primary'
      @iconName='open-in-new'
      {{style fontSize='1.5714rem !important'}}
      data-test-storeknoxInventoryDetails-viewNamespacesLinkOpenInNewIcon
    />
  </:leftIcon>

  <:default>
    {{t 'notificationModule.viewNamespaces'}}
  </:default>
</AkLink>