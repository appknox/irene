{{#if @data.available}}
  {{#if (eq @data.available 'VAPT')}}
    <Storeknox::Indicator
      @svgComponent='ak-svg/vapt-indicator'
      @indicatorInfo={{t 'storeknox.vaptIndicatorText' htmlSafe=true}}
    />
  {{else}}
    <Storeknox::Indicator
      @svgComponent='ak-svg/sm-indicator'
      @indicatorInfo={{t 'storeknox.smIndicatorText' htmlSafe=true}}
    />
  {{/if}}
{{else}}
  <Storeknox::Indicator
    @svgComponent='ak-svg/info-indicator'
    @indicatorInfo={{t 'storeknox.infoIndicatorWhitelabelText' htmlSafe=true}}
  />
{{/if}}