{{#if this.showTable}}
  <Storeknox::Discover::PendingReview::Table
    @columns={{this.columns}}
    @data={{this.reviewAppsData}}
    @limit={{this.limit}}
    @offset={{this.offset}}
    @loadingData={{this.loadingData}}
    @totalCount={{this.totalCount}}
    @goToPage={{this.goToPage}}
    @onItemPerPageChange={{this.changePerPageItem}}
  />
{{else}}
  <Storeknox::Discover::PendingReview::Empty />
{{/if}}