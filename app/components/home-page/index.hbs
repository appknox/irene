{{#if this.isLoaded}}
  {{#if this.isSecurityDashboard}}
    <HomePage::SecurityDashboardNav
      @user={{@user}}
      @logoutAction={{this.invalidateSession}}
    />

    {{yield}}
  {{else}}
    <HomePage::OrganizationDashboard
      @isSecurityEnabled={{this.isSecurityEnabled}}
      @user={{@user}}
      @logoutAction={{this.invalidateSession}}
    >
      {{yield}}
    </HomePage::OrganizationDashboard>
  {{/if}}
{{/if}}

{{#if this.showAddEditModal}}
  <OrganizationNameHeader::AddEditModal
    @organization={{this.organization.selected}}
    @handleCancel={{this.handleCancel}}
  />
{{/if}}