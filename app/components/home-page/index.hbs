{{#if this.isLoaded}}
  {{#if this.isSecurityDashboard}}
    <div class='columns security-dashboard'>
      <div class='column top-nav'>
        <div class='columns top-columns'>
          <div class='column'>
            <div class='appknox-logo'>
              <ImgLogo />
            </div>
          </div>
          <div class='column content-right top-icons'>
            <a href='#'>
              {{fa-icon 'user' class='sidebar-icons'}}
              {{@user.username}}
              &nbsp;

              {{fa-icon 'angle-down' class='sidebar-icons'}}

              <AttachPopover
                @interactive={{true}}
                @placement='bottom'
                @showOn='click'
                @hideOn='clickout'
              >
                <div class='user-info-popover'>
                  <div class='user-name'>
                    {{@user.username}}
                  </div>
                  <div class='user-email'>
                    {{@user.email}}
                  </div>
                  <div class='content-right'>
                    {{! template-lint-disable no-nested-interactive }}
                    <a
                      href='#'
                      class='mp-auth-logout highlighted-button'
                      {{on 'click' this.invalidateSession}}
                    >
                      {{fa-icon 'power-off' class='sidebar-icons'}}
                      {{t 'logout'}}
                    </a>
                  </div>
                </div>

              </AttachPopover>

            </a>
          </div>
        </div>
      </div>
    </div>
    <SecuritySplit />
  {{else}}
    <div class='columns columns-main-container'>
      <div class='column is-one-sixth side-menu'>
        <div class='side-menu-fixed'>
          <div class='appknox-logo'>
            <ImgLogo />
          </div>
          <ul class='menu-list text-center margin-top20'>
            <li>
              <LinkTo
                @route='authenticated.projects'
                class="class='mp-auth-projects"
              >
                <div>
                  {{fa-icon 'tasks' class='sidebar-icons'}}{{t 'projects'}}
                </div>
                <span class='badge-square is-sidebar content-right'>
                  {{this.organization.selected.projectsCount}}
                </span>
              </LinkTo>
            </li>{{#if this.isShowAnalytics}}
              <li>

                <LinkTo @route='authenticated.analytics'>
                  <div>
                    {{fa-icon 'area-chart' class='sidebar-icons'}}{{t
                      'analytics'
                    }}
                  </div>
                </LinkTo>

              </li>
            {{/if}}<li>
              <LinkTo
                @route='authenticated.organization.namespaces'
                class='mp-auth-teams'
              >
                <div>
                  {{fa-icon 'users' class='sidebar-icons'}}{{t 'organization'}}
                </div>
              </LinkTo>
            </li>
            <li>
              <LinkTo @route='authenticated.settings' class='mp-auth-setting'>
                <div>
                  {{fa-icon 'cog' class='sidebar-icons'}}{{t 'accountSettings'}}
                </div>
              </LinkTo>

            </li>
            {{#if this.showMarketplace}}
              <li>

                <LinkTo
                  @route='authenticated.marketplace'
                  class='mp-auth-marketplace'
                >
                  <div>
                    {{fa-icon 'cubes' class='sidebar-icons'}}{{t 'marketplace'}}
                  </div>
                </LinkTo>

              </li>
            {{/if}}{{#if this.showBilling}}
              <li>
                <LinkTo @route='authenticated.billing' class='mp-auth-billing'>
                  <div>
                    {{fa-icon 'tag' class='sidebar-icons'}}{{t 'billing'}}
                  </div>
                </LinkTo>
              </li>
            {{/if}}{{#if this.showPartnerDashboard}}
              <li>
                <LinkTo
                  @route='authenticated.partner.clients'
                  class='mp-auth-clients'
                >
                  <div>
                    {{fa-icon 'sitemap' class='sidebar-icons'}}{{t
                      'clients'
                    }}<sup class='margin-left primary-color'>
                      beta
                    </sup>
                  </div>
                </LinkTo>

              </li>
            {{/if}}
          </ul>
          <div class='status-updates'>
            <SubmissionList />
          </div>
          <div
            class='product-version
              {{if this.enablePendo "ak-pendo-version-container"}}'
          >
            {{t 'version'}}
            {{this.productVersion}}
          </div>
        </div>
      </div>
      <div class='column margin-right margin-top'>
        <div class='columns'>
          <div class='column top-nav'>
            <div class='columns top-columns'>
              <div class='column'>
                <div class='columns'>
                  <div class='scan-text'>
                    {{t 'startNewScan'}}
                  </div>
                  <div class='column'>
                    <label
                      disabled={{this.isUploading}}
                      class='button is-primary ac-upload-app highlighted-button'
                    >

                      <UploadApp delegate={{this}} class='mp-upload-app' />
                      {{fa-icon 'upload'}}&nbsp;
                      {{#if this.isUploading}}
                        {{t 'uploading'}}&nbsp;:{{this.progress}}%
                      {{else}}
                        {{t 'uploadApp'}}
                      {{/if}}
                    </label>
                  </div>
                </div>
              </div>
              <div class='column content-right top-icons'>
                {{#if this.isSecurityEnabled}}
                  <a
                    href='/security/projects'
                    target='_blank'
                    class='button security-link'
                    rel='noopener noreferrer'
                  >
                    Security Dashboard
                  </a>
                {{/if}}
                {{#if this.showKnowledgeBase}}
                  <a href='#' {{on 'click' this.onOpenKnowledgeBase}}>
                    {{fa-icon 'book' class='sidebar-icons'}}Knowledge Base
                  </a>
                {{/if}}
                <a href='#' {{on 'click' this.openChatBox}}>
                  {{fa-icon 'support' class='sidebar-icons'}}{{t 'support'}}
                </a>
                <a href='#'>
                  {{fa-icon 'user' class='sidebar-icons'}}{{@user.username}}
                  &nbsp;{{fa-icon 'angle-down' class='sidebar-icons'}}

                  <AttachPopover
                    @interactive={{true}}
                    @placement='bottom'
                    @showOn='click'
                    @hideOn='clickout'
                  >
                    <div class='user-info-popover'>
                      <div class='user-name'>
                        {{@user.username}}
                      </div>
                      <div class='user-email'>
                        {{@user.email}}
                      </div>
                      <div class='content-right'>
                        {{! template-lint-disable no-nested-interactive }}
                        <a
                          href='#'
                          class='mp-auth-logout highlighted-button'
                          {{on 'click' this.invalidateSession}}
                        >
                          {{fa-icon 'power-off' class='sidebar-icons'}}{{t
                            'logout'
                          }}
                        </a>
                      </div>
                    </div>
                  </AttachPopover>
                </a>
              </div>
            </div>
          </div>
        </div>
        <div class='margin-top50'>
          {{yield}}
        </div>
      </div>
    </div>
  {{/if}}
{{/if}}

<ModalCard
  @isActive={{this.isEmptyOrgName}}
  @title={{t 'modalCard.orgModal.title'}}
  @class='input-modal'
>
  <div class='card-body'>
    <div class='card-wrapper'>
      <form>
        <p class='control'>
          <Input
            placeholder={{t 'modalCard.orgModal.title'}}
            @type='text'
            @value={{this.organizationName}}
            autofocus='autofocus'
            class='form-control input input-width margin-left'
            id='organizationName'
          />
        </p>
        <button
          disabled={{this.isUpdatingOrg}}
          class='is-primary margin-left margin-top10'
          {{on 'click' (perform this.updateOrgName)}}
          type='button'
        >
          {{#if this.isUpdatingOrg}}
            <div class='fa-font-size'>
              <i class='fa fa-spinner fa-spin'></i>
            </div>
            &nbsp;
          {{/if}}{{t 'update'}}
        </button>
      </form>
    </div>
  </div>
</ModalCard>