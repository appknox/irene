  <AkTypography @fontWeight='bold' local-class='sub-header'>
    {{t 'plugins'}}
  </AkTypography>

  <AkTypography local-class='sub-header-description'>
    {{t 'marketplacePluginDesc'}}
  </AkTypography>
</AkStack>

