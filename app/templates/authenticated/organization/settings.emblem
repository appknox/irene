.org-container
  = organization-settings organization=model.organization user=model.user

  .columns
    .column
      .org-settings-subtitle.black-text.is-fullwidth
        = t "jiraIntegration"
      = jira-account user=user
  .divider.margin-t-1.margin-b-1
  .columns
    .column
      .org-settings-subtitle.black-text.is-fullwidth
        = t "githubIntegration"
      = github-account integratedUser=model.integratedUser reconnect=model.reconnect user=user

  .divider.margin-t-1.margin-b-1
    = multisso-account
