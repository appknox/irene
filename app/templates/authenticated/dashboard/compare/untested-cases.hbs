{{page-title (t 'untested')}}

<FileCompare::Table::AnalysesProvider
  @comparisonFilterKey={{@model.comparisonFilterKey}}
  @files={{@model.files}}
  as |tableCtx|
>
  {{#if tableCtx.isLoadingAnalyses}}
    <FileCompare::Loader @loadingText='{{t "loading"}}...' />
  {{else}}
    <FileCompare::Table
      @comparisonFilterKey={{@model.comparisonFilterKey}}
      @files={{@model.files}}
      @filteredComparisons={{tableCtx.filteredComparisons}}
    />
  {{/if}}
</FileCompare::Table::AnalysesProvider>