  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.{css,scss}\" --allow-empty-input",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "cross-env IRENE_API_HOST=http://localhost:8000 ember server",
    "startstaging": "cross-env IRENE_API_HOST=https://api.appknox.io IRENE_DEVICEFARM_URL=wss://devicefarm.appknox.com ember server",
    "startprod": "cross-env IRENE_API_HOST=https://api.appknox.com IRENE_DEVICEFARM_URL=wss://devicefarm.appknox.com ember server",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "deploy": "ember deploy production --verbose=true --activate=true --show-progress=true",
    "deploy:server": "ember deploy server",
    "storybook": "start-storybook -p 6006 -s dist --ci",
    "storybook-dev": "concurrently \"npm:start\" \"npm:storybook\" --names \"dev-server,storybook\"",
    "prebuild-storybook": "ember build",
    "build-storybook": "build-storybook -s dist"
  },
